{
  "name": "elm-ts-interop-starter",
  "version": "1.0.0",
  "description": "Starter repo for elm-ts-interop.",
  "main": "index.js",
  "scripts": {
    "start": "elm-live src/Main.elm -- --output elm.js",
    "generate": "elm-ts-interop-pro --entrypoint Main --definitions src/InteropDefinitions.elm --output src/Main/index.d.ts --gen-directory gen",
    "generate:watch": "chokidar \"src/**/*.elm\" --initial --command \"npm run generate\"",
    "lint": "eslint index.js"
  },
  "author": "Dillon Kearns",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@incrementalelm/elm-ts-interop-pro": "^0.0.21",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chokidar-cli": "^2.1.0",
    "elm-live": "^4.0.2",
    "elm-ts-interop": "^0.0.3",
    "eslint": "^7.13.0",
    "typescript": "^4.0.5"
  }
}