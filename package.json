{
  "name": "elm-ts-interop-starter",
  "version": "1.0.0",
  "description": "Starter repo for elm-ts-interop.",
  "main": "index.js",
  "scripts": {
    "start": "vite --port 1234",
    "build": "npm run generate && tsc --noEmit && npm run lint && vite build",
    "generate": "elm-ts-interop --definition-module InteropDefinitions --output src/Main.elm.d.ts",
    "lint": "eslint index.ts",
    "postinstall": "elm-tooling install"
  },
  "author": "Dillon Kearns",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "elm-tooling": "^1.6.0",
    "eslint": "^8.0.0",
    "prettier": "^2.4.1",
    "tiny-decoders": "^6.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.7",
    "vite-plugin-elm": "^2.5.0"
  }
}